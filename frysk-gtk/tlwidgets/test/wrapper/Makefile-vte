   CLASSPATH=.:/usr/lib/jvm/java-1.4.2-gcj-1.4.2.0/jre/lib/rt.jar:/usr/share/java/gtk2.8.jar:/usr/share/java/glib0.2.jar:/usr/share/java/vte0.11.jar

   LD_LIBRARY_PATH=/usr/local/qt/lib:/usr/local/lib

all::run

VTETest.class: VTETest.java
	CLASSPATH=$(CLASSPATH) gcj -C $<

run: VTETest.class
	LD_LIBRARY_PATH=$(LD_LIBRARY_PATH) gij -cp $(CLASSPATH) VTETest

clean:
	rm -f *.class
