      <title></title>
      <figure id=predefinedcolors>
	<title></title>
	<mediaobject>
	  <imageobject>
	    <imagedata
	      fileref="colorwheel.jpg"
	      format=JPG
	      scale=40
	      align=center>
	  </imageobject>
	  <caption>
	    <para>
	      The predefined colors used by the viewer.
	    </para>
	  </caption>
	</mediaobject>
      </figure>
