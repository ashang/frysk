   CLASSPATH=.:.:/opt/frysk/lib:/usr/lib/jvm/java-1.4.2-gcj-1.4.2.0/jre/lib/rt.jar:/opt/frysk/share/java/gtk2.8.jar:/opt/frysk/share/java/cairo1.0.jar:/opt/frysk/share/java/glib0.2.jar:/opt/frysk/share/java/vte0.11.jar

   LD_LIBRARY_PATH=/usr/local/qt/lib:/usr/local/lib:/opt/frysk/lib:

all::run

VTETest.class: VTETest.java
	CLASSPATH=$(CLASSPATH) gcj -C $<

run: VTETest.class
	LD_LIBRARY_PATH=$(LD_LIBRARY_PATH) gij -cp $(CLASSPATH) VTETest

clean:
	rm -f *.class
